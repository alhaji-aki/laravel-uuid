{
    "name": "alhaji-aki/laravel-uuid",
    "description": "Trait to generate uuid when creating models",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "uuid"
    ],
    "autoload": {
        "psr-4": {
            "AlhajiAki\\LaravelUuid\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlhajiAki\\LaravelUuid\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "AlhajiAki\\LaravelUuid\\UuidServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Abdul Kudus",
            "email": "abdulkudus2922@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/database": "^7.30|^8.0",
        "illuminate/support": "^7.30|^8.0"
    },
    "require-dev": {
        "ext-pdo_sqlite": "*",
        "friendsofphp/php-cs-fixer": "^3.0",
        "orchestra/testbench": "^5.16|^6.11",
        "phpunit/phpunit": "^9.5.1"
    },
    "scripts": {
        "test": "phpunit",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
